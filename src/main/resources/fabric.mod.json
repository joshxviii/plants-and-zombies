{
	"schemaVersion": 1,
	"id": "plantz",
	"version": "${version}",
	"name": "Plants & Zombies",
	"description": "Find and grow helpful Plants that can defend your base.",
	"authors": [
		"joshxviii"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/plantz/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"value": "joshxviii.plantz.PazMain",
				"adapter": "kotlin"
			}
		],
		"client": [
			{
				"value": "joshxviii.plantz.PazClient",
				"adapter": "kotlin"
			}
		]
	},
	"mixins": [
		"plantz.mixins.json",
		{
			"config": "plantz.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.18.4",
		"minecraft": "~26.1-",
		"java": ">=25",
		"fabric-api": "*",
		"fabric-language-kotlin": "*"
	}
}